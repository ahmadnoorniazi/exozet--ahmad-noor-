{
  "name": "react-hot-boilerplate",
  "version": "1.0.0",
  "description": "Boilerplate for ReactJS project with hot code reloading",
  "scripts": {
    "server": "webpack-dev-server",
    "webpack": "webpack",
    "lint": "eslint \"src/**/*.{js,jsx}\"",
    "clean": "rimraf dist/",
    "build": "npm run webpack -- --env.mode production",
    "stats": "npm run webpack -- --env.presets stats",
    "dev": "npm run server -- --env.mode development",
    "pretty": "npx prettier  --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "eslint-fix": "eslint --fix \"src/**/*.{js,jsx}\"",
    "test": "jest --coverage",
    "test:watch": "jest --watchAll",
    "setup": "npm install && npm run test && npm run dev"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./src/setupTests.js"
  },
  "keywords": [
    "react",
    "reactjs",
    "boilerplate",
    "hot",
    "reload",
    "hmr",
    "live",
    "edit",
    "webpack"
  ],
  "license": "MIT",
  "devDependencies": {
